{
    "all": true,
    "include": [
        "**/use-cases/**",
        "**/domain-collectors/**",
        "**/ws-api/**",
        "**/workers/**"
    ],
    "exclude": [
        "**/tests/**",
        "**/use-cases/BaseUseCase.js",
        "**/use-cases/utils/**",
        "**/use-cases/testing/**",
        "**/ws-api/*.*",
        "**/workers/*.*",
        "**/workers/**/*worker.*"
    ],
    "reporter": ["text", "html"],
    "check-coverage": true,
    "branches": 80,
    "lines": 80,
    "functions": 80,
    "statements": 80
}
