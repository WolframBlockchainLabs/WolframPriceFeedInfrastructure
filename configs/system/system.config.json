{
    "appPort": "{{PORT}}",
    "appTestPort": 3002,
    "appMode": "{{NODE_ENV}}",
    "appTrustProxy": "{{APP_TRUST_PROXY}}",
    "mainUrl": "{{MAIN_URL}}",
    "projectName": "{{PROJECT_NAME}}",
    "db": {
        "username": "{{DB_USERNAME}}",
        "password": "{{DB_PASSWORD}}",
        "database": "{{DB_DATABASE}}",
        "dialect": "{{DB_DIALECT}}",
        "host": "{{DB_HOST}}",
        "port": "{{DB_PORT}}"
    },
    "test-db": {
        "username": "{{TEST_DB_USERNAME}}",
        "password": "{{TEST_DB_PASSWORD}}",
        "database": "{{TEST_DB_DATABASE}}",
        "dialect": "{{TEST_DB_DIALECT}}",
        "host": "{{TEST_DB_HOST}}",
        "port": "{{TEST_DB_PORT}}"
    },
    "rabbitmq": {
        "urls": [
            "amqp://{{RABBITMQ_USER}}:{{RABBITMQ_PASS}}@{{RABBITMQ_HOST}}:5672/"
        ]
    },
    "logger": {
        "isPlainText": "{{LOGGER_PLAIN_TEXT}}",
        "level": "{{LOGGER_LEVEL}}",
        "transports": [
            {
                "type": "console",
                "pretty": true
            }
        ]
    },
    "intervals": {
        "marketsRefresher": "{{MARKETS_REFRESHER_INTERVAL}}"
    },
    "amqpWorker": {
        "retryLimit": 12,
        "retryPeriodMs": 3600000
    },
    "aggregateOHLCVEmitter": {
        "interval": "{{AGGREGATE_OHLCV_EMITTER_INTERVAL}}",
        "exchanges": [
            "Binance",
            "Bitfinex",
            "Bitget",
            "Bitstamp",
            "Bybit",
            "Gate.io",
            "Gemini",
            "Kraken",
            "KuCoin",
            "OKX"
        ],
        "pairs": ["BTC/EUR", "BTC/USDT", "ETH/USDT", "ETH/EUR", "LTC/BTC"]
    }
}
