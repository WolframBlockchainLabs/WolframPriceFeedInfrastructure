{
  "name": "ccdb",
  "type": "module",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "node --max-old-space-size=8192 app.js",
    "nodemon": "nodemon --max-old-space-size=8192 app.js",
    "tasks:news": "MODE=application node --max-old-space-size=8192 lib/workers/news/app.js run",
    "docker:up": "docker-compose -f docker/docker-compose.yml --project-name skeleton up",
    "docker:build": "docker-compose -f docker/docker-compose.yml --project-name skeleton build",
    "docker:exec:be": "docker exec -it skeleton-be /bin/sh",
    "docker:exec:db": "docker exec -it skeleton-db /bin/sh",
    "docker:down": "docker-compose -f docker/docker-compose.yml --project-name skeleton down",
    "docker:test:ava": "docker exec -i skeleton-be npm run test:ava",
    "docker:test:coverage": "docker exec -i skeleton-be npm run test:coverage",
    "docker:test:lint": "docker exec -i skeleton-be npm run test:lint",
    "docker:test:audit": "docker exec -i skeleton-be npm run test:audit",
    "docker:seed:database": "docker exec -i skeleton-be npm run seed:database",
    "docker:migration:db": "docker exec -i skeleton-be npm run migration:db",
    "docker:migration:test": "docker exec -i skeleton-be npm run migration:test",
    "docker:migration:rollback:db": "docker exec -i skeleton-be npm run migration:rollback:db",
    "docker:migration:rollback:test": "docker exec -i skeleton-be npm run migration:rollback:test",
    "docker:migration:reset": "npm run docker:migration:rollback:db && npm run docker:migration:db && npm run docker:migration:rollback:test && npm run docker:migration:test && npm run docker:seed:database",
    "test:lint": "eslint lib tests app.js --ext js",
    "test:ava": "ava --verbose",
    "test:coverage": "c8 npm run test:ava",
    "test:audit": "npm audit --audit-level=moderate",
    "test": "npm run test:lint && npm run test:audit && npm run test:coverage",
    "seed:database": "MODE=application node --max-old-space-size=8192 seeders/Database.js run",
    "migration:db": "sequelize db:migrate --config lib/config.cjs --env db",
    "migration:test": "sequelize db:migrate --config lib/config.cjs --env test-db",
    "migration:rollback:db": "sequelize db:migrate:undo --config lib/config.cjs --env db",
    "migration:rollback:test": "sequelize db:migrate:undo --config lib/config.cjs --env test-db",
    "ci:wait-mysql": "MODE=test node --max-old-space-size=8192 seeders/Ci.js"
  },
  "pre-commit": [
    "test:lint"
  ],
  "dependencies": {
    "bluebird": "^3.7.2",
    "bull": "^3.22.0",
    "busboy": "^1.0.0",
    "chista": "^1.0.0",
    "cls-hooked": "^4.2.2",
    "commander": "^7.0.0",
    "confme": "^1.0.0",
    "connect-session-sequelize": "^7.1.1",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "docopt": "^0.6.2",
    "express": "^4.17.1",
    "express-prom-bundle": "^6.3.6",
    "express-session": "^1.17.2",
    "express-useragent": "^1.0.15",
    "fs-extra": "^8.1.0",
    "handlebars": "^4.7.6",
    "handlebars-json-beauty": "^1.0.0",
    "is-secret": "^1.2.1",
    "json-pointer": "^0.6.2",
    "jsonwebtoken": "^8.4.0",
    "livr": "^2.3.1",
    "livr-extra-rules": "^1.1.0",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "multer": "^1.4.5-lts.1",
    "node-2fa": "^2.0.1",
    "pg": "^8.10.0",
    "pg-hstore": "^2.3.4",
    "prom-client": "^13.1.0",
    "rename-keys": "^2.0.1",
    "request-ip": "^2.1.3",
    "sendmail": "^1.6.1",
    "sequelize": "^6.6.5",
    "sequelize-cli": "^6.5.2",
    "sequelize-slugify": "^1.5.0",
    "sharp": "^0.30.7",
    "transliteration": "^2.2.0",
    "traverse": "^0.6.6",
    "uuid": "^8.3.2",
    "winston": "^3.3.3",
    "winston-daily-rotate-file": "^4.5.5",
    "winston-transport": "^4.4.0"
  },
  "devDependencies": {
    "ava": "^3.8.2",
    "babel-eslint": "^10.1.0",
    "c8": "^7.1.2",
    "cookie": "^0.4.1",
    "cookie-signature": "^1.1.0",
    "eslint": "^7.1.0",
    "eslint-config-webbylab": "^5.3.1",
    "eslint-plugin-ava": "^10.3.0",
    "eslint-plugin-fetch": "0.0.1",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-more": "1.0.0",
    "eslint-plugin-no-require-lodash": "^1.1.0",
    "eslint-plugin-prefer-spread": "^1.0.3",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-security": "^1.4.0",
    "faker": "^4.1.0",
    "form-data": "^2.5.0",
    "mkcert": "^1.5.1",
    "node-fetch": "^2.6.7",
    "nodemon": "^2.0.20",
    "pre-commit": "^1.2.2",
    "query-string": "^7.0.0"
  },
  "description": "NodeJS Backend CCDB",
  "main": "app.js",
  "repository": {
    "type": "git",
    "url": "https://gitlab-ee.webbylab.com/cc-blockchain-labs/exchange-analyzer/backend"
  },
  "author": "WebbyLab"
}
